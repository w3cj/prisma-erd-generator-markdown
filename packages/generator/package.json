{
  "name": "prisma-erd-generator-markdown",
  "description": "Generate an ERD in a markdown file from a prisma schema.",
  "version": "1.3.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-erd-generator-markdown": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "@prisma/generator-helper": "4.11.0",
    "prettier": "2.8.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.15.3",
    "@types/prettier": "2.7.2",
    "jest": "29.5.0",
    "prisma": "4.11.0",
    "semantic-release": "^20.1.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "homepage": "https://github.com/w3cj/prisma-erd-generator-markdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/w3cj/prisma-erd-generator-markdown"
  },
  "author": "CJ R. <cj@null.computer>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
